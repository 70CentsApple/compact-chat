{
  "text.autoconfig.compact-chat.title": "Compact Chat Options",

  "text.autoconfig.compact-chat.category.default": "General Settings",

  "text.autoconfig.compact-chat.option.onlyCompactConsecutiveMessages": "Only compact consecutive messages",
  "text.autoconfig.compact-chat.option.onlyCompactConsecutiveMessages.@Tooltip[0]": "Messages will only be compacted if",
