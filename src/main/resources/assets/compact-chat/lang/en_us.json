  "text.autoconfig.compact-chat.option.onlyCompactConsecutiveMessages.@Tooltip[4]": " ",
  "text.autoconfig.compact-chat.option.onlyCompactConsecutiveMessages.@Tooltip[5]": "Example when enabled: ",
  "text.autoconfig.compact-chat.option.onlyCompactConsecutiveMessages.@Tooltip[6]": "  <Player>: hi",
  "text.autoconfig.compact-chat.option.onlyCompactConsecutiveMessages.@Tooltip[7]": "  <Player>: hey",
  "text.autoconfig.compact-chat.option.onlyCompactConsecutiveMessages.@Tooltip[8]": "  <Player>: hi <- Will not be compacted!",
  "text.autoconfig.compact-chat.option.onlyCompactConsecutiveMessages.@Tooltip[9]": "Example when disabled (default):",
  "text.autoconfig.compact-chat.option.onlyCompactConsecutiveMessages.@Tooltip[10]": "  <Player>: hi",
  "text.autoconfig.compact-chat.option.onlyCompactConsecutiveMessages.@Tooltip[11]": "  <Player>: hey",
  "text.autoconfig.compact-chat.option.onlyCompactConsecutiveMessages.@Tooltip[12]": "  <Player>: hi <- Will be compacted!"
